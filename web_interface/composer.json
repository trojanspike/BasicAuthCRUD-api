{
	"name": "TS-easycrud-api",
	"description": " An easy to use Crud and rest API, cross domain JS request made easy ",
	"keywords": ["api", "content", "management"],
	"license": "MIT",
	"require": {
		"php" : ">=5.4.0",
	   "leafo/scssphp": "0.0.12",
	   "illuminate/database": "5.0.4"
	},
	"require-dev": {},
	"autoload": {
	    "psr-4": {
	    	"App\\":"core/Classes/App/",
	    	"Database\\":"core/Classes/Database/",
	    	"Model\\":"core/Classes/Model/",
	    	"Illuminate\\Database\\": ""
	    },
		"classmap": ["../src/"],
		"files": ["core/GlobalFn.php"]
	},
	"autoload-dev": {},
	"scripts": {
		"post-install-cmd": [],
		"post-update-cmd": [],
		"post-create-project-cmd": []
	},
	"config": {
	    "vendor-dir": "./vendor/",
		"preferred-install": "dist"
	},
	"minimum-stability": "stable",
	"_Reference" : "https://getcomposer.org/doc/04-schema.md"
}
